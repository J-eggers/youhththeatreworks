 <section id="posts">
    <div class="container">
      <div class="row">
        <div class="col">
          <div class="card">
            <div class="card-header">
              <h4>Cast for {{show.title}}</h4>
            </div>
            <div class="table-responsive">
              <table class="table table-striped">
                <thead class="nav-Theme">
                  <tr>
                    <th>#</th>
                    <th>Name</th>
                    <th>phone Number</th>
                    <th class="resp-hidden">email</th>
                    <th>Parent Name</th>
                  </tr>
                </thead>
                {{#each show.cast}}
                <tbody>
                  <tr>
                    <td>{{@index}}</td>
                    <td>
                      <a href="/kids/kidprofile/{{kidID}}" class="text-gold">
                      {{kidname}}</a>
                    </td> 
                    <td>{{phone}}</td>
                    <td class="resp-hidden">{{email}}</td>
                    <td>{{parentName}}</td>
                  </tr>
                </tbody>
                {{/each}}
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>